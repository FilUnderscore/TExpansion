package me.fil.craftedmc.modpack;

import java.io.File;

public abstract class ModPack implements IModPack
{
	Version version;
	String[] mods;
	String[] authors;
	boolean isThirdParty;
	File jarFile;
	File directory;
	
	protected ModPack(Version version, String[] mods, String[] authors, boolean isThirdParty, File jarFile, File directory)
	{
		this.version = version;
		this.mods = mods;
		this.authors = authors;
		this.isThirdParty = isThirdParty;
		this.jarFile = jarFile;
		this.directory = directory;
	}
	
	protected void func_388212_a(int p_388271_a)
	{
		
	}
}
